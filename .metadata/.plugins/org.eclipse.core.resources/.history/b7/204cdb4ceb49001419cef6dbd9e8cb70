package com.NaN.nutracker;

import java.io.Serializable;

public class Stop implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 3855823764029423373L;
	public String stopID;
	public boolean selected;
	public Store storeInfo;

	public Stop() {
		super();
	}

	public Stop(String stopID, boolean selected, Store storeInfo) {
		super();
		this.stopID = stopID;
		this.selected = selected;
		this.storeInfo = storeInfo;
	}
	
	public Store getStoreInfo() {
		return storeInfo;
	}

	public void setStoreInfo(Store storeInfo) {
		this.storeInfo = storeInfo;
	}

	public String getStopID() {
		return stopID;
	}

	public boolean isSelected() {
		return selected;
	}

}
